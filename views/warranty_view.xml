<?xml version="1.0" encoding="utf-8" ?>
<odoo>

     <record id="view_patient_tag_tree" model="ir.ui.view">
        <field name="name">warranty.status.tree</field>
        <field name="model">warranty.status</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>

    </record>


<!--    form-->
    <record id="view_warranty_form" model="ir.ui.view">
        <field name="name">warranty.status.form</field>
        <field name="model">warranty.status</field>
        <field name="arch" type="xml">
            <form>

                <sheet>
                    <group>
                        <field name="name"/>
                    </group>

                </sheet>


            </form>
        </field>

    </record>

<!--    action-->
    <record id="action_warranty" model="ir.actions.act_window">
        <field name="name">Tags</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">warranty.status</field>
        <field name="view_mode">tree,form</field>
        <field name="context">{}</field>
        <field name="help" type="html">
            <p class="">create warranty status</p>
        </field>
    </record>

<!--    submeu-->
    <menuitem id="menu_patient_tag"
              name="Tags"
              action="action_warranty"
              parent="configuration_main_menu"
              sequence="100"/>


</odoo>