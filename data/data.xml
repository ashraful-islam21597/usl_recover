<odoo>
    <record id="seq_field_service" model="ir.sequence">
        <field name="name">Field Service</field>
        <field name="code">field.service</field>
        <field name="prefix">SO</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>


     <record id="seq_field_service_data" model="ir.sequence">
        <field name="name">Field Service Data</field>
        <field name="code">field.service.data</field>
        <field name="prefix">IMEI</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>


</odoo>